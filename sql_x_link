SELECT a.uid, a.person, a.target_person, a.x_link, b.uid, b.person, b.target_person, b.x_link  FROM `tx_relax5core_domain_model_relation` a 
LEFT JOIN `tx_relax5core_domain_model_relation` b ON a.person = b.target_person


UPDATE `tx_relax5core_domain_model_relation` a 
JOIN `tx_relax5core_domain_model_relation` b ON a.person = b.target_person SET a.x_link = b.uid, b.x_link = a.uid

